<h2 class="text-2xl font-bold mb-4">Districts</h2>
<table class="min-w-full bg-white">
  <thead>
    <tr class="w-full bg-gray-200 text-gray-600 uppercase text-sm leading-normal">
      <th class="py-3 px-6 text-left">Name</th>
      <th colspan="3" class="py-3 px-6 text-center">Actions</th>
    </tr>
  </thead>
  <tbody class="text-gray-600 text-sm font-light">
    <% @districts.each do |district| %>
      <tr class="border-b border-gray-200 hover:bg-gray-100">
        <td class="py-3 px-6 text-left"><%= district.name %></td>
        <td class="py-3 px-6 text-center">
          <%= link_to 'Show', district, class: 'text-blue-600 hover:underline' %>
        </td>
        <td class="py-3 px-6 text-center">
          <%= link_to 'Edit', edit_district_path(district), class: 'text-yellow-600 hover:underline' %>
        </td>
        <td class="py-3 px-6 text-center">
          <%= button_to 'Destroy', district, method: :delete, data: { confirm: 'Are you sure?' }, class: 'text-red-600 hover:underline' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New District', new_district_path, class: 'inline-block mt-4 px-4 py-2 bg-blue-600 text-white font-semibold rounded-lg hover:bg-blue-700' %>
