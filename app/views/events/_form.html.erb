<%= form_with(model: event, local: true) do |form| %>
    <% if event.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(event.errors.count, "error") %> prohibited this event from being saved:</h2>
        <ul>
          <% event.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
  
    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
  
    <div class="field">
      <%= form.label :start_date %>
      <%= form.date_field :start_date %>
    </div>
  
    <div class="field">
      <%= form.label :end_date %>
      <%= form.date_field :end_date %>
    </div>
  
    <div class="field">
      <%= form.label :district_id %>
      <%= form.collection_select :district_id, District.all, :id, :name %>
    </div>
  
    <div class="field">
      <%= form.label :county_id %>
      <%= form.collection_select :county_id, County.all, :id, :name %>
    </div>
  
    <div class="field">
      <%= form.label :sub_county_id %>
      <%= form.collection_select :sub_county_id, SubCounty.all, :id, :name %>
    </div>
  
    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
  